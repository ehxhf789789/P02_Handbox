# AWS Credentials
# AWS 콘솔 (IAM > 사용자 > 보안 자격 증명)에서 발급
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=ap-southeast-2

# S3 Configuration
S3_BUCKET_NAME=cnt-evaluation-system
S3_RAW_PREFIX=raw/
S3_PROCESSED_PREFIX=processed/

# Bedrock Configuration
BEDROCK_REGION=us-east-1
# Claude 3.5 Sonnet 사용
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20240620-v1:0
# Titan Embeddings 사용
BEDROCK_EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# OpenSearch Serverless Configuration
OPENSEARCH_ENDPOINT=https://your-collection-id.ap-southeast-2.aoss.amazonaws.com
OPENSEARCH_INDEX_NAME=cnt-vectors

# Optional: 로컬 테스트용 (Bedrock 대신 OpenAI 사용)
# OPENAI_API_KEY=your_openai_key_here
